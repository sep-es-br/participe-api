		<query language="sql"><![CDATA[WITH $P{ID_CONFERENCE} AS Conference_Id,
		$P{ID_MICROREGION} AS Microregion_Id
CALL {
  WITH Microregion_Id
  MATCH (microrregion:Locality)<-[:IS_LOCATED_IN]-(city:Locality)
  WHERE id(microrregion) = Microregion_Id
  RETURN microrregion, reduce(cities = '', c IN collect(city.name) | CASE cities WHEN '' THEN c ELSE cities + ', '+ c END) as cities
} WITH microrregion, cities, Conference_Id
CALL { WITH Conference_Id
  MATCH (planItem:PlanItem)-[:COMPOSES]->(plan:Plan)<-[:TARGETS]-(conf:Conference)
  WHERE ID(conf) = Conference_Id
  RETURN planItem as planItem
} WITH microrregion, cities, planItem
CALL {
	WITH microrregion, planItem
	MATCH (microrregion)<-[:ABOUT|IS_LOCATED_IN*1..2]-(a:Comment)-[:ABOUT]->(:PlanItem)-[:COMPOSES]->(planItem)
	WHERE (NOT a.status IN ['rem', 'pen'])
	RETURN 
		count(distinct a) as quantityComment
}
CALL {
	WITH microrregion, planItem
	MATCH
		(microrregion)<-[:ABOUT|IS_LOCATED_IN*1..2]-(a:Highlight)-[:ABOUT]->(:PlanItem)-[:COMPOSES]->(planItem)
	RETURN
		count(distinct a) as quantityHighlight
}
WITH planItem, quantityComment, quantityHighlight, microrregion, cities
RETURN microrregion.name AS microregion, 
  cities, planItem.name as planItemName, quantityComment, quantityHighlight
ORDER BY planItemName]]></query>
